		"vercel": "^25.2.0",
		"webdriver-manager": "^12.1.9"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"copyfiles": "2.4.1",
		"d3": "7.9.0",
		"eslint": "^9.39.1",
		"eslint-formatter-gitlab": "^6.0.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"wrangler": "^4.53.0"
	}
