{
	"name": "nabla-site-apache",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"description": "Simple HTML project for Nabla company promoting Alban Andrieu as an experienced DevSecOps",
	"main": "index.html",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlbanAndrieu/nabla-site-apache.git"
	},
	"author": "Alban Andrieu (aandrieu)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AlbanAndrieu/nabla-site-apache/issues"
	},
	"homepage": "https://github.com/AlbanAndrieu/nabla-site-apache#readme",
	"engines": {
		"pnpm": "^10.22.0",
		"node": ">=24.11.0",
		"npm": ">=11.6.2"
	},
	"scripts": {
		"postinstall": "copyfiles -f ./node_modules/d3/d3.min.js ./index/dist/scripts/d3 && copyfiles -f ./app/scripts/arf.js ./dist/scripts/ && copyfiles -f ./app/resources/*.json ./dist/resources && copyfiles -f ./index.html ./dist/ && copyfiles -f ./wireframe.css ./dist/",
		"lint": "eslint --format gitlab .",
		"audit": "npm audit --audit-level=moderate",
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"start-python": "python -m http.server 8001 --directory public",
		"hugo:build": "hugo --minify",
		"hugo:dev": "hugo server -D",
		"hugo:clean": "npx rimraf resources/_gen hugo_stats.json .hugo_build.lock",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:debug": "playwright test --debug",
		"test:report": "playwright show-report",
		"build:includes": "node scripts/build-html-includes.js"
	},
	"dependencies": {
		"@astrojs/vercel": "^9.0.2",
		"@datadog/browser-rum": "^6.24.1",
		"crypto": "^1.0.1",
		"lodash-es": "^4.17.23",
		"stripe": "^20.2.0",
		"vercel": "^50.18.0",
		"webdriver-manager": "^12.1.9"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@playwright/test": "^1.58.0",
		"copyfiles": "2.4.1",
		"d3": "7.9.0",
		"eslint": "^9.39.1",
		"eslint-formatter-gitlab": "^6.0.1",
		"opencommit": "^3.2.10",
		"playwright": "^1.58.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"wrangler": "^4.59.1"
	}
}
