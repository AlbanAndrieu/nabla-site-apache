		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"start-python": "python -m http.server 8001"
	},
	"dependencies": {
		"@datadog/browser-rum": "^6.24.1",
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
