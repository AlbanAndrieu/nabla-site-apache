{
	"version": 2,
	"functions": {
		"api/**/*.php": {
			"runtime": "vercel-php@0.7.4",
			"excludeFiles": "{.next,*.cache,.node_cache,.cursor,.ansible,node_modules,.venv,.direnv,public,reports/2025/*,reports/2026/*,index/*.bmp}/**"
		}
	},
	"routes": [
		{ "src": "/api/honeypot/(.*)", "dest": "/api/login/$1" },
		{ "src": "/(.*)", "dest": "/api/$1" }
	],
	"env": {
		"NOW_PHP_FOO": "bar3"
	},
	"build": {
		"env": {
			"NOW_PHP_DEBUG": "1"
		}
	}
}
